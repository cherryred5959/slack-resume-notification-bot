{
    "name": "cherryred5959/slack-resume-notification-bot",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Cherryred5959\\SlackResumeNotificationBot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cherryred5959\\SlackResumeNotificationBot\\Tests\\": "tests/"
        }
    },
    "require": {
        "workerman/workerman": "^3.5",
        "vlucas/phpdotenv": "^3.3",
        "mpociot/slack-client": "^1.2",
        "fabpot/goutte": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "fzaninotto/faker": "^1.8"
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
}
